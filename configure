    if test "$DISABLE_SCRIPTING" = TRUE; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: no, overridden by --disable-scripting" >&5
$as_echo "no, overridden by --disable-scripting" >&6; }
        with_dotnet=no
        ENABLE_DOTNET=""
    else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
        ENABLE_DOTNET=TRUE
    fi
else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    ENABLE_DOTNET=""
fi

if test "$ENABLE_DOTNET" = TRUE; then
    if test -n "$with_dotnet" && test "$with_dotnet" != yes; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for dotnet" >&5
$as_echo_n "checking for dotnet... " >&6; }
        PathFormat "$with_dotnet"
        DOTNET="$formatted_path_unix"

        if test -f "$DOTNET"; then
                        { $as_echo "$as_me:${as_lineno-$LINENO}: result: $formatted_path" >&5
$as_echo "$formatted_path" >&6; }
        else

            as_fn_error $? "$DOTNET is not a valid dotnet executable" "$LINENO" 5
            ENABLE_DOTNET=""
        fi
    else

        if test "$OS" = WNT
        then
                for ac_prog in dotnet.exe
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_DOTNET+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $DOTNET in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_DOTNET="$DOTNET" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_DOTNET="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
DOTNET=$ac_cv_path_DOTNET
if test -n "$DOTNET"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $DOTNET" >&5
$as_echo "$DOTNET" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$DOTNET" && break
done

        else
                for ac_prog in dotnet
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_DOTNET+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $DOTNET in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_DOTNET="$DOTNET" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
