    # WARNING: any option with an argument containing spaces must be handled separately (see --with-theme)
    sub_conf_defaults=" \
        --build="$build_alias" \
        --disable-cairo-canvas \
        --disable-cups \
        --disable-customtarget-components \
        --disable-firebird-sdbc \
