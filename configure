do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_DOTNET="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
DOTNET=$ac_cv_path_DOTNET
if test -n "$DOTNET"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $DOTNET" >&5
$as_echo "$DOTNET" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$DOTNET" && break
done

        fi
        PathFormat "$DOTNET"
        DOTNET="$formatted_path_unix"

        if test -z "$DOTNET"; then

            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: dotnet not found, disabling .NET support" >&5
$as_echo "$as_me: WARNING: dotnet not found, disabling .NET support" >&2;}
            ENABLE_DOTNET=""
        fi
    fi

    if test -n "$DOTNET"; then

        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for .NET SDK 8 or higher" >&5
$as_echo_n "checking for .NET SDK 8 or higher... " >&6; }
        dotnet_sdk_ver_major=`"$DOTNET" --version | "$AWK" -F. '{ print $1 }'`
        if test "$dotnet_sdk_ver_major" -ge 8; then

            dotnet_sdk_ver=`"$DOTNET" --version | "$SED" 's/\r//g'`
            dotnet_sdk_dir=`"$DOTNET" --list-sdks | "$AWK" -F [][] "/^$dotnet_sdk_ver/"'{ print $2 }' | "$SED" 's/\r//g'`
            PathFormat "$dotnet_sdk_dir"
            DOTNET_ROOT=`dirname "$formatted_path_unix"`

            { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
        else

            { $as_echo "$as_me:${as_lineno-$LINENO}: result: no, disabling .NET support" >&5
$as_echo "no, disabling .NET support" >&6; }
            ENABLE_DOTNET=""
        fi
    fi
fi

