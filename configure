$as_echo "no, $CCACHE_VERSION" >&6; }
                    CCACHE=""
                    CCACHE_DEPEND_MODE=
                fi
            fi
        fi
        if test "$enable_ccache" = yes && test -z "$CCACHE"; then
            as_fn_error $? "No suitable ccache found" "$LINENO" 5
        fi
        ;;
    esac
else
    CCACHE=""
fi
if test "$enable_ccache" = "nodepend"; then
    CCACHE_DEPEND_MODE=""
