                { $as_echo "$as_me:${as_lineno-$LINENO}: sccache seems to be included in a pre-defined CC and/or CXX" >&5
$as_echo "$as_me: sccache seems to be included in a pre-defined CC and/or CXX" >&6;}
                CCACHE_DEPEND_MODE=1
                SCCACHE=1
                ;;
            *)
