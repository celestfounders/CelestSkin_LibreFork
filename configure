    test_cpdb=no
    test_cups=no
    test_dbus=no
    test_gen=no
    test_gstreamer_1_0=no
    test_gtk3_kde5=no
    test_gtk3=no
    test_gtk4=no
    test_kf5=no
    test_kf6=no
    test_qt5=no
    test_qt6=no
    test_randr=no
    test_xrender=no
}

test "$using_x11" = yes && USING_X11=TRUE

if test "$using_freetype_fontconfig" = yes; then
    $as_echo "#define USE_HEADLESS_CODE 1" >>confdefs.h

    USE_HEADLESS_CODE=TRUE
    if test "$using_headless_plugin" = yes; then
        $as_echo "#define ENABLE_HEADLESS 1" >>confdefs.h

        ENABLE_HEADLESS=TRUE
    fi
else
    test_fontconfig=no
    test_freetype=no
fi




