    if test -n "$LO_CLANG_CC" -a -n "$LO_CLANG_CXX" -a "$_os" = "WINNT"; then
        save_CXX="$CXX"
        CXX="$LO_CLANG_CXX"
        { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether $CXX supports -Zc:dllexportInlines-" >&5
$as_echo_n "checking whether $CXX supports -Zc:dllexportInlines-... " >&6; }
        ac_ext=cpp
