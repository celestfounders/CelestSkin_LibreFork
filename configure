  $as_echo_n "(cached) " >&6
else
  case $ANT in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_ANT="$ANT" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
